<p id="notice"><%= notice %></p>

<h1>Admin Page</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>




        <tr>
          <td><%= link_to user.email, user_path(user) %></td>
        </tr>
        <tr>
          <td><%= link_to 'Edit', edit_user_path(user) %></td>
        </tr>

        <tr>
          <td><%= link_to 'Delete User', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>

        </tr>

    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
